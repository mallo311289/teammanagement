{
  "expo": {
    "name": "TeamTrack",
    "slug": "teamtrack-moorgreen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/moorgreen-colts-fc-logo.webp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/moorgreen-colts-fc-logo.webp",
      "resizeMode": "contain",
      "backgroundColor": "#4A6FA5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.teamtrack.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./public/moorgreen-colts-fc-logo.webp",
        "backgroundColor": "#4A6FA5"
      },
      "package": "com.teamtrack.app"
    },
    "web": {
      "favicon": "./public/moorgreen-colts-fc-logo.webp",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
